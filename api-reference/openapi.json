    "/api/v1/modify-options": {
      "get": {
        "summary": "Modify AI-generated content options",
        "parameters": [
          {
            "name": "jobId",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "option",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "enum": [
                "longer",
                "shorter",
                "casual",
                "serious",
                "original"
              ]
            }
          },
          {
            "name": "aiPart",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "enum": [
                "introduction",
                "titles",
                "shortTitles",
                "summary",
                "keywords",
                "keyTopicsWithBullets",
                "faq",
                "webArticleSEOFriendly",
                "emailNewsletter",
                "threadsByInstagram",
                "youtubeDescription",
                "facebookPost",
                "twitterThread",
                "linkedInPost",
                "clipFinder",
                "reelScript"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ModifyOptionsSuccessResponse"
